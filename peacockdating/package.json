{
  "name": "peacockdating",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx tailwindcss -i ./static/tailwind.input.css -o ./static/site.css --watch",
    "build": "node ./scripts/copy-assets.mjs && npx tailwindcss -i ./static/tailwind.input.css -o ./static/site.css --minify",
    "serve": "npx http-server ./static -p 5173 -c-1",
    "start": "npm run build && npm run serve",
    "deploy": "npm run build && gh-pages -d static"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1",
    "tailwindcss": "^3.4.13"
  }
}
